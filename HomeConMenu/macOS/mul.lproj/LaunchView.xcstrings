    "Htv-O8-lHD.title" : {
      "comment" : "Class = \"NSTextFieldCell\"; title = \"Open HomeConMenu from the Menu bar and then you can turn on/off switches, change the color of lightbulbs, and so on.\"; ObjectID = \"Htv-O8-lHD\";",
