    "0MS-0l-x61.title" : {
      "comment" : "Class = \"NSTextFieldCell\"; title = \"You can not find any devices on the menu if you do not have any homes or add any home devices to your “Home” on your HomeKit. You can add a HomeKit device using Home app on iOS devices.\"; ObjectID = \"0MS-0l-x61\";",
      "extractionState" : "extracted_with_value",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Wenn es kein Zuhause in Ihrer HomeKit-Umgebung gibt oder dieses keine Geräte enthält, wird beim Öffnen von HomeConMenu nichts angezeigt. Verwenden Sie die Home-App auf Ihrem iPhone, um Ihre Geräte zu HomeKit hinzuzufügen."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "new",
            "value" : "You can not find any devices on the menu if you do not have any homes or add any home devices to your “Home” on your HomeKit. You can add a HomeKit device using Home app on iOS devices."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "あなたのHomeKit環境にHomeがなかったり、Homeにホームデバイスが追加されていない場合、HomeConMenuを開いても何も表示されません。iOSのホームアプリを使って、あなたのホームデバイスをHomeKitに追加してください。"
          }
        }
      }
    },
